<script>
  function createChart(chartId, chartColor) {
    const options = {
      series: [
        { name: "series1", data: [18, 25, 22, 40, 34, 55, 50, 60, 55, 65] },
      ],
      chart: {
        type: "area",
        width: 80,
        height: 42,
        sparkline: { enabled: true },
        toolbar: { show: false },
      },
      dataLabels: { enabled: false },
      stroke: { curve: "smooth", width: 1, colors: [chartColor] },
      grid: { show: false },
      fill: {
        type: "gradient",
        colors: [chartColor],
        gradient: {
          shade: "light",
          type: "vertical",
          opacityFrom: 0.5,
          opacityTo: 0.3,
          stops: [0, 100],
        },
      },
      markers: { colors: [chartColor], size: 0, hover: { size: 8 } },
      xaxis: { labels: { show: false }, tooltip: { enabled: false } },
      yaxis: { labels: { show: false } },
      tooltip: { x: { format: "dd/MM/yy HH:mm" } },
    };

    const chartEl = document.querySelector(`#${chartId}`);
    if (chartEl) {
      new ApexCharts(chartEl, options).render();
    }
  }

  // Create charts for each dashboard box
  createChart("graphic-design", "#2FB2AB");
  createChart("web-dev", "#27CFA7");
  createChart("digital-marketing", "#6142FF");
  createChart("others", "#FA902F");
  createChart("total-colleges", "#2FB2AB");
  createChart("total-courses", "#27CFA7");
  createChart("total-students", "#6142FF");
</script>
